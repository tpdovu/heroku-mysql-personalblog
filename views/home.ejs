<%- include partials/header.ejs %>

<h1>Home</h1>
<p> <%- homeStartingContent %> </p>


<% posts.slice().reverse().forEach(post => { %>
<h3><%= post.title %> </h3>
<h6><%= post.date_created %></h6>
<p class="truncate"> <%= post.body %> </p>
<a href="/posts/<%=post.title%>/id/<%=post.id%>">Read More</a>
<% }) %>

<%- include partials/footer.ejs %>